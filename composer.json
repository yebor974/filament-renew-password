{
    "name": "yebor974/filament-renew-password",
    "description": "Package for manage renew password according to the last renew or other criteria",
    "keywords": [
        "laravel",
        "filament",
        "renew",
        "password"
    ],
    "homepage": "https://github.com/yebor974/filament-renew-password",
    "support": {
        "issues": "https://github.com/yebor974/filament-renew-password/issues",
        "source": "https://github.com/yebor974/filament-renew-password"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Julien BOYER",
            "email": "contact@julienboyer.re"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yebor974\\Filament\\RenewPassword\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yebor974\\Filament\\RenewPassword\\FilamentRenewPasswordServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
